<h1 class="title">Saved Profiles</h1>

{{#each users}}

<section class='profiel'>
    <div>
        <img class='accounts' src="/images/girl.jpg" alt="">
        <h1>{{firstName}}</h1>
        <h2>{{lastName}}</h2>
        <p>{{age}} years</p>
        <p>{{location}} away</p>
        <a href="/chat"><button class=profiel>Start Chatting</button></a>
        <form action="/savedprofiles" method="POST">
            <button class="skipButton" type="submit" name="userid2" value={{_id}}> <img id="likeknop"
                    src="images/skipknop.png" </button>
        </form>
    </div>
</section>



{{/each}}

